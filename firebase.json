{
  "functions": { "source": ".output/server" },
  "hosting": [
    {
      "site": "rcc-interactive-blog",
      "public": ".output/public",
      "cleanUrls": true,
      "rewrites": [{ "source": "**", "function": "server" }],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ]
}
